<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="CustodianLife.Model" namespace="CustodianLife.Model">
  <class name="TreatyProportion" table="TBIL_TREATY_PROPTN">
    <id name="tpId" column="TBIL_TREATY_PROPTN_REC_ID"  >
      <generator class="identity"/>
    </id>
    <property name="SystemModule" column="TBIL_TRTY_PROP_MDLE" length="3" />
    <property name="TreatyYear" column="TBIL_TRTY_PROP_YR" type="integer" />
    <property name="TreatyCompany" column="TBIL_TRTY_PROP_COMP_CD" length="3" />
    <property name="TreatyType" column="TBIL_TRTY_PROP_TYPE" length="1"  />
    <property name="StartSumAssuredValue" column="TBIL_TRTY_PROP_ST_AMT" type="decimal" />
   
  </class>
</hibernate-mapping>
