<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="CustodianLife.Model" namespace="CustodianLife.Model">
  <class name="ReInsuranceTreatyCommission" table="TBIL_TREATY_COMM">
    <id name="tcId" column="TBIL_TREATY_COMM_REC_ID"  >
      <generator class="identity"/>
    </id>
    <property name="SystemModule" column="TBIL_TREATY_COMM_MDLE" length="3" />
    <property name="TreatyCommYear" column="TBIL_TREATY_COMM_YR" type="integer" />
    <property name="TreatyType" column="TBIL_TREATY_COMM_TYPE" length="1"  />
    <property name="ProductCode" column="TBIL_TREATY_COMM_PRDCT_CD" length="3"  />
    <property name="SumAssuredStartPeriod" column="TBIL_TREATY_COMM_ST_RRD" type="integer" />
    <property name="SumAssuredEndPeriod" column="TBIL_TREATY_COMM_END_PRD" type="integer" />
    <property name="StartSumAssuredAmt" column="TBIL_TREATY_COMM_ST_AMT" type="Decimal" />
    <property name="EndSumAssuredAmt" column="TBIL_TREATY_COMM_END_AMT" type="Decimal" />
    <property name="CommissionRate" column="TBIL_TREATY_COMM_RATE" type="Decimal"  />

  </class>
</hibernate-mapping>
