<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="CustodianLife.Model" namespace="CustodianLife.Model">
  <class name="BonusInterestRate" table="TBIL_BONUS_INTREST">
    <id name="brId" column="TBIL_BONUS_INTREST_REC_ID"  >
      <generator class="identity"/>
    </id>
    <property name="ProductCode" column="TBIL_BONUS_RT_PRDCT_CD" length="3" />
    <property name="BonusRateType" column="TBIL_BONUS_RT_CODE" length="3"/>
    <property name="BonusYear" column="TBIL_BONUS_RT_YEAR" type="integer" />
    <property name="BonusRate" column="TBIL_BONUS_RATE" type="decimal" />
    <property name="BonusRatePer" column="TBIL_BONUS_RATE_PER" type="decimal" />
  </class>
</hibernate-mapping>
