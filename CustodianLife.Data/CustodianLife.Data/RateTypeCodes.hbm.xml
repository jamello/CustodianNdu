<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="CustodianLife.Model" namespace="CustodianLife.Model">
  <class name="RateTypeCodes" table="TBIL_RATE_TYPE_CODE">
    <id name="rtId" column="TBIL_RATE_TYPE_CODE_REC_ID" >
      <generator class="identity"/>
    </id>
    <property name="ModuleSource" column="TBIL_RATE_TYP_MDLE" length="1" />
    <property name="ProductCode" column="TBIL_RATE_TYP_PRDCT" length="3" />
    <property name="RateTypeCode" column="TBIL_RATE_TYP_CODE" length="3" />
    <property name="RateTypeDesc" column="TBIL_RATE_TYP_DESC" length="300" />
    <property name="RatePer" column="TBIL_RATE_TYP_PER" type="integer" />

  </class>
</hibernate-mapping>