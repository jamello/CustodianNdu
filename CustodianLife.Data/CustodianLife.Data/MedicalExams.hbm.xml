    <id name="meId" column="TBIL_MEDIC_EXAM_REC_ID" >
      <generator class="identity"/>
    </id>
    <property name="ModuleSource" column="TBIL_MEDIC_EXAM_MDLE" length="1" />
    <property name="StartSumAssured" column="TBIL_MEDIC_SA_FROM" type="decimal" />
    <property name="EndSumAssured" column="TBIL_MEDIC_SA_TO" type="decimal" />
    <property name="StartAge" column="TBIL_MEDIC_AGE_FROM" type="integer" />
    <property name="EndAge" column="TBIL_MEDIC_AGE_TO" type="integer" />
    <property name="ProductCode" column="TBIL_MEDIC_PRDCT_CD" length="3" />

  </class>
</hibernate-mapping>